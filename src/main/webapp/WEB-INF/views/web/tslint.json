{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "service-worker.js",
      "static/**/*.js",
      "lib/**/*.js"
    ]
  },
  
  "rules": {
        "max-line-length": {
            "options": [240]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "off",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "no-reference": false
        ,"max-classes-per-file":10
        ,"object-literal-sort-keys":false
    }
    ,
    "jsRules": {
        "max-line-length": {
            "options": [120]
        },
        "object-literal-sort-keys": false
        
    }
   
}
